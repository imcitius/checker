{
  "name": env.INSTANCE_NAME,
  "project_name": env.CI_PROJECT_NAME,
  "auto_deploy": true,
  "overwrite": true,
  "datacenters": [env.INSTANCE_DC],
  "deploy_repository_url": "git::ssh://git@jgit.me/instantiator/tests/guide-deploy",
  "params": {
    "image": env.REGISTRY_IMAGE,
    "version": env.CI_COMMIT_REF_NAME,
    "major_version": env.CI_COMMIT_REF_SLUG | split("-") as $major | $major,
    "gitlab_project": env.CI_PROJECT_URL,
    "commit_sha": env.CI_COMMIT_SHA,
    "commit_message": env.CI_COMMIT_MESSAGE,
    "fail": "false",
    "run_time": ""
  }
}